{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "Components/Hero/Hero.jsx", "Components/Header.Tsx", "Components/Logo/Logo.jsx", "Components/NavBar/NavBar.tsx", "Components/Services/Services.jsx", "Components/About/About.jsx", "Components/Projects/Projects.jsx", "Components/Contact/Contact.jsx", "Components/Footer/Footer.jsx"],
  "exclude": ["node_modules"]
}
